# Cluster config secrets
# Copy to cluster-config-secrets.yaml and fill in values
cloudflare:
  apiToken: your-cloudflare-api-token

homarr:
  oidc:
    clientSecret: your-homarr-oidc-client-secret
  dbEncryptionKey: your-homarr-db-encryption-key

garage:
  adminToken: your-garage-admin-token  # Generate with: openssl rand -hex 32

rclone:
  accessKeyId: your-garage-access-key-id
  secretAccessKey: your-garage-secret-access-key
